{
    "name": "infinite-scroll-list",
    "version": "1.0.0",
    "description": "Приложение для отображения списка элементов с бесконечным скроллом",
    "main": "src/index.tsx",
    "scripts": {
        "start": "webpack serve --env mode=development",
        "build:dev": "webpack --env mode=development",
        "build:prod": "webpack --env mode=production",
        "build:analyze": "webpack --env mode=development analyzer=true",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "lint:fix": "eslint \"**/*.{ts,tsx}\" --fix",
        "format": "prettier --write \"**/*.{ts,tsx,scss,css,json}\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.26.0",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
        "@svgr/webpack": "^8.1.0",
        "@testing-library/jest-dom": "^6.1.4",
        "@testing-library/react": "^14.1.2",
        "@testing-library/react-hooks": "^8.0.1",
        "@testing-library/user-event": "^14.5.1",
        "@types/html-webpack-plugin": "^3.2.9",
        "@types/jest": "^29.5.5",
        "@types/node": "^22.8.1",
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@types/testing-library__jest-dom": "^6.0.0",
        "@types/webpack": "^5.28.5",
        "@types/webpack-dev-server": "^4.7.2",
        "@typescript-eslint/eslint-plugin": "^8.11.0",
        "@typescript-eslint/parser": "^8.11.0",
        "axios": "^1.7.7",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "eslint": "^9.13.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.2",
        "eslint-plugin-react-hooks": "^5.0.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "html-webpack-plugin": "^5.6.3",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "mini-css-extract-plugin": "^2.9.1",
        "prettier": "^3.3.3",
        "react-refresh": "^0.14.2",
        "react-refresh-typescript": "^2.0.9",
        "sass": "^1.80.4",
        "sass-loader": "^16.0.2",
        "style-loader": "^4.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.3",
        "webpack": "^5.95.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.1.0"
    },
    "author": "https://github.com/GrinOnFace",
    "license": "MIT",
    "dependencies": {
        "@emotion/react": "^11.13.3",
        "@emotion/styled": "^11.13.0",
        "@mui/material": "^6.1.5",
        "mobx": "^6.13.5",
        "mobx-react-lite": "^4.0.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.27.0"
    },
    "jest": {
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
            "<rootDir>/src/setupTests.ts"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^@/(.*)$": "<rootDir>/src/$1"
        },
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        }
    }
}
